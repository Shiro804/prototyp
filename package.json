{
  "name": "projektseminar",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "seed": "tsx data/seed.ts",
    "seed2": "tsx data/seed2.ts",
    "resetandseed": "npx prisma db push --force-reset && npx prisma migrate dev --name init && npx prisma generate && npm run seed",
    "resetandseed2": "npx prisma db push --force-reset --schema=prisma/schema2.prisma && npx prisma migrate dev --name init --schema=prisma/schema2.prisma && npx prisma generate --schema=prisma/schema2.prisma && npm run seed2",
    "migrate": "npx prisma migrate dev --name init"
  },
